<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="empty" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <head>
        <!-- Bootstrap -->
        <css src="Cmsmart_Marketplace::vendors/bootstrap/css/bootstrap.min.css"/>
        <!-- Font Awesome -->
        <css src="Cmsmart_Marketplace::vendors/font-awesome/css/font-awesome.min.css"/>
        <!-- NProgress -->
        <css src="Cmsmart_Marketplace::vendors/nprogress/nprogress.css"/>
        <!-- bootstrap-daterangepicker -->
        <css src="Cmsmart_Marketplace::vendors/bootstrap-daterangepicker/daterangepicker.css"/>

        <!-- Custom Theme Style -->
        <css src="Cmsmart_Marketplace::build/css/admin_vendor.min.css"/>
    </head>
    <body>
	    <attribute name="class" value="nav-md"/>
        <referenceBlock name="page.main.title" remove="true"/>
        <referenceContainer name="content">
            <container name="vendor.panel.wrapper" htmlClass="vendor-panel" htmlTag="div" before="-">
                <block class="Cmsmart\Marketplace\Block\Vendor" name="marketplace.vendor.admin" template="Cmsmart_Marketplace::vendor/index.phtml" cacheable="false">
                    <block class="Cmsmart\Marketplace\Block\Vendor\Dashboard" name="marketplace.vendor.content" template="Cmsmart_Marketplace::vendor/dashboard.phtml" cacheable="false"/>
                </block>
            </container>
        </referenceContainer>
    </body>
</page>