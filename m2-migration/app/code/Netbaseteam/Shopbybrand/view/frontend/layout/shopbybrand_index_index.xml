<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>   
        <script src="Netbaseteam_Shopbybrand::js/list.js" />
	</head>
    <body>
		
        <attribute name="class" value="emizen_brand_index"/>
        <referenceContainer name="content"> 
            <block class="Netbaseteam\Shopbybrand\Block\Index" name="brand.list.content"  template="Netbaseteam_Shopbybrand::cmsmart_shopbybrand/list.phtml" cacheable="false" ifconfig="shopbybrand/general/Enabled">
             </block>
        </referenceContainer>
		<referenceContainer name="sidebar.additional">
            <block class="Magento\Wishlist\Block\Customer\Sidebar" name="wishlist_sidebar" as="wishlist" template="Magento_Wishlist::sidebar.phtml"/>
        </referenceContainer>
		
    </body>
</page>
